{
  "description": "Send Clover and LCOV coverage reports to the Coveralls service.",
  "homepage": "https://docs.belin.io/coveralls.php",
  "license": "MIT",
  "name": "cedx/coveralls",
  "type": "library",
  "authors": [
    {"email": "cedric@belin.io", "homepage": "https://belin.io", "name": "CÃ©dric Belin"}
  ],
  "autoload": {
    "psr-4": {"Coveralls\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"Coveralls\\": "test/"}
  },
  "bin": [
    "bin/coveralls"
  ],
  "config": {
    "optimize-autoloader": true
  },
  "funding": [
    {"type": "patreon", "url": "https://www.patreon.com/cedx"}
  ],
  "keywords": [
    "client",
    "clover",
    "code",
    "coverage",
    "coveralls",
    "lcov"
  ],
  "require": {
    "php": ">=7.4.0",
    "ext-date": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pcre": "*",
    "ext-SimpleXML": "*",
    "ext-spl": "*",
    "cedx/lcov": "^8.2.0",
    "cedx/which": "^9.0.0",
    "nyholm/psr7": "^1.2.1",
    "psr/http-client": "^1.0.0",
    "symfony/console": "^4.4.8 || ^5.0.8",
    "symfony/event-dispatcher": "^4.4.8 || ^5.0.8",
    "symfony/http-client": "^5.0.8",
    "symfony/mime": "^5.0.8",
    "symfony/yaml": "^4.4.8 || ^5.0.8",
    "webmozart/path-util": "^2.3.0"
  },
  "require-dev": {
    "consolidation/robo": "^2.0.3",
    "phpstan/phpstan": "^0.12.23",
    "phpunit/phpunit": "^9.1.4"
  },
  "scripts": {
    "coverage": "robo coverage",
    "test": "robo test"
  },
  "support": {
    "docs": "https://api.belin.io/coveralls.php",
    "issues": "https://git.belin.io/cedx/coveralls.php/issues"
  }
}
